<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.StackPane?>

<StackPane id="AnchorPane" fx:id="pnlAlbum" onMouseClicked="#abrirCanciones" prefHeight="165.0" prefWidth="166.0" stylesheets="@../styles/panelCss.css" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controladores.TarjetaAlbumController">
    <children>
        <StackPane style="-fx-background-color: #D1F2EB;">
         <GridPane>
           <columnConstraints>
             <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
           </columnConstraints>
           <rowConstraints>
             <RowConstraints maxHeight="135.0" minHeight="10.0" prefHeight="123.0" vgrow="SOMETIMES" />
             <RowConstraints maxHeight="75.0" minHeight="10.0" prefHeight="42.0" valignment="CENTER" vgrow="SOMETIMES" />
           </rowConstraints>
            <children>
                  <ImageView fx:id="imgAlbum" fitHeight="121.0" fitWidth="174.0" pickOnBounds="true" preserveRatio="true">
                  <GridPane.margin>
                     <Insets left="10.0" right="10.0" top="10.0" />
                  </GridPane.margin>
               </ImageView>
                  <Label fx:id="lblNombreAlbum" text="Label" GridPane.rowIndex="1" />
            </children>
         </GridPane>
        </StackPane>
    </children>
</StackPane>
