<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="50.0" prefWidth="200.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controladores.TarjetaMenuCancionController">
     <children>
      <VBox prefHeight="50.0" prefWidth="200.0">
         <children>
            <JFXButton fx:id="btnAgregarCola" onAction="#agregarCola" prefWidth="200.0" text="Agregar a cola">
               <VBox.margin>
                  <Insets />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </JFXButton>
            <JFXButton fx:id="btnAgregarLista" onAction="#agregarLista" prefWidth="200.0" text="Agregar a PlayList">
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
            </JFXButton>
         </children>
      </VBox>
   </children>
</AnchorPane>
